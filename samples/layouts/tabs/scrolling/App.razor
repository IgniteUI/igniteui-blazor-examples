@using IgniteUI.Blazor.Controls


<div class="container vertical">
    <IgbTabs>
        @for(int i=1; i<=20; i++)
        {
            string tabName = "Tab " + i.ToString();
            string panelName = "Panel" + i.ToString();
            string panelContent = "Tab Panel " + i.ToString();

            <IgbTab Panel=@panelName>@tabName</IgbTab>
            <IgbTabPanel id=@panelName>@panelContent</IgbTabPanel>
        }
    </IgbTabs>
</div>

